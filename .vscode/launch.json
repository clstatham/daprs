    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch `demo example`",
            "program": "${workspaceFolder}/target/debug/examples/demo.exe",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch `bang` example",
            "program": "${workspaceFolder}/target/debug/examples/bang.exe",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch `buffer` example",
            "program": "${workspaceFolder}/target/debug/examples/buffer.exe",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'raug'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=raug"
                ],
                "filter": {
                    "name": "raug",
                    "type": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
    ]
}